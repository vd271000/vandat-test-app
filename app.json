{
  "expo": {
    "name": "vandat-test-app",
    "slug": "vandat-test-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./resources/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.vandattestapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./resources/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.vandattestapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./resources/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
